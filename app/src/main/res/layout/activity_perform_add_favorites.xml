<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".performAddFavActivity">

    <TextView
        android:id="@+id/title_favorited_done_textview"
        android:layout_width="276dp"
        android:layout_height="58dp"
        android:text="TextView"
        android:textAppearance="@style/TextAppearance.AppCompat.Body1"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/imageView2"
        tools:layout_editor_absoluteY="85dp" />

    <ImageView
        android:id="@+id/imageView2"
        android:layout_width="60dp"
        android:layout_height="60dp"
        android:layout_marginTop="16dp"
        android:src="@drawable/common_google_signin_btn_icon_light"
        tools:layout_editor_absoluteX="28dp"
        tools:layout_editor_absoluteY="85dp" />

    <TextView
        android:id="@+id/title_favorited_done_uid_textview"
        android:layout_width="206dp"
        android:layout_height="35dp"
        android:layout_marginTop="32dp"
        android:text="TextView"
        android:textSize="24sp"
        app:layout_constraintEnd_toEndOf="@+id/title_favorited_done_textview"
        app:layout_constraintHorizontal_bias="0.571"
        app:layout_constraintStart_toStartOf="@+id/title_favorited_done_textview"
        app:layout_constraintTop_toBottomOf="@+id/title_favorited_done_textview" />
</androidx.constraintlayout.widget.ConstraintLayout>