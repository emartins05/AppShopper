<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.appshopper">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label=""
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">


        <activity android:name=".LatestItensActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".registration.LoginActivity" />

        <activity android:name=".registration.RegisterActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".performAddFavActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LatestItensActivity" />

        </activity>
        <activity android:name=".FavoritedActivity">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".LatestItensActivity" />
        </activity>

    </application>

</manifest>